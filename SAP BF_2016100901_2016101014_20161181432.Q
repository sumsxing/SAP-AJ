[Script]
Hwnd = Plugin.Window.Find("SAP_FRONTEND_SESSION", 0)
If hwnd = 0 Then 
    MessageBox "SAP未启动，请先启动SAP"
    ExitScript 
End If
Call Plugin.Window.Active(Hwnd)
Call Plugin.Window.Max(Hwnd)
SaveMousePos
MoveTo 400, 400
Hwndex = Plugin.Window.MousePoint()
Delay 200
Call Plugin.Window.Min(Hwnd)
RestoreMousePos
Call Plugin.Media.Vol(99)
RT = Cint(form1.RT.text)
DT = Cint(Form1.DT.Text)
Do Until X > 0
    TracePrint Hwnd
    Call Plugin.Bkgnd.KeyPress(Hwnd, 116)
    Delay DT * 1000
    Call Plugin.Media.Beep(523, 200)
    Delay 1000
    Call Plugin.Window.Active(Hwnd)
    Call Plugin.Window.Max(Hwnd)
    Delay 50
    FindColorEx 100,265,500,500,"666666",0,1,X,Y
    Call Plugin.Window.Min(Hwnd)
    If X > 0 and Y > 0 Then 
        Call Plugin.Media.Play("C:\Windows\Media\tada.wav")
        Delay 1000
    Else 
        Call Plugin.Media.Beep(523, 200)
        Delay RT * 60000
    End If
Loop
If form1.CheckBox1.Value = 1 Then 
    Call Plugin.Window.Max(Hwnd)
    Delay 500
    MoveTo X, Y
    Delay 500
    LeftClick 1
    Delay 500
    KeyPress 120, 1
    Delay 2000
    KeyDown 17, 1
    KeyPress 116, 1
    KeyUp 17, 1
End If
If form1.CheckBox2.Value = 1 Then 
    Call Plugin.Bkgnd.KeyPress(Hwndex, 33)
    Delay 500
    Call Plugin.Bkgnd.KeyPress(Hwndex, 120)
    Delay 2000
End If